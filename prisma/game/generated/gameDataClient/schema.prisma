generator client {
  provider = "prisma-client-js"
  output   = "./generated/gameDataClient"
}

datasource gamedb {
  provider = "mysql"
  url      = env("DATABASE_URL1")
}

model item {
  id         Int    @id @default(autoincrement())
  item_code  Int    @unique
  item_name  String
  health     Int
  power      Int
  item_price Int

  @@map("items")
}
